<!DOCTYPE html>
<html>
  <head>
    <title>Smsmanager</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="hd">
        <div id="nav">
          <%= link_to "Users" ,"/users" %> |
          <%= link_to "Companies" ,"/companies" %> |
          <%= link_to "SMS Templates" ,"/templates" %> |
          <%= link_to "Upload Data" ,"/appointment" %> |
          <%= link_to "Appointment" ,"/appointment/report" %> 
        </div>
    </div>
    <%= yield %>
  </body>
</html>
